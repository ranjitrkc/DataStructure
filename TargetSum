using System;

public class TargetSum
{
    
    public static void Main(string[] args)
    {
        int[] arr = new int[5];
        
        for(int i = 0; i< arr.Length; i++)
            arr[i] = int.Parse(Console.ReadLine());
        
        int left = 0, right = arr.Length-1;
        int target = int.Parse(Console.ReadLine());
        
        while(right>left)
        {
            //loop values based on sum
            int sum = arr[right] +arr[left];
            
            if(sum == target)
                Console.WriteLine("sum of {0} and {1}", arr[left], arr[right] );
                
            //if sum of right and left is greater than sum, then right--    
            if(sum>target)
                right--;
             
            //if sum of right and left is lesser than sum, then left++   
            else
                left++;
        }
    }
}
