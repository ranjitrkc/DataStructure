using System;
using System.Collections.Generic;

public class SubArraySumMultOfK
{
    public static void Main(string[] args)
    {
        int[] nums = {23,2,4,6,7};
        int k = 6, count = 0, currentSum = 0, size = 2, remainder = 0, subArrLength = 0;
        bool flag = false;
        
        Dictionary<int, int> map = new Dictionary<int, int>();
        map[0] = -1;
        
        for(int i =0; i<nums.Length; i++)
        {
            currentSum += nums[i];
            remainder = currentSum%k;

            if(map.ContainsKey(remainder))
            {
                subArrLength = i - map[remainder];
                if(subArrLength >= size)
                    flag = true;
            }
            else
                map[remainder] = i;
        }
        
        Console.WriteLine(flag);
    }
}
